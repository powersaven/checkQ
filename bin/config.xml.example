    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.schoonerinfotech.com/configSchema rules.xsd ">

<servers>
    <write ip="127.0.0.1" port="3306" />
    <read ip="127.0.0.1" port="3306" />
</servers>
<tables>
    <table>
        <key name="a" type="bigint" />
        <val name="b" type="varchar" length="10" />
    </table>
</tables>
<cmds>
    <cmd>
        <insert />
        <update />
        <select />
    </cmd>
</cmds>

</rs:definition>
